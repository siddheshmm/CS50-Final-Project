{
    "manifest_version": 2,
    "name": "Currency Converter",
    "version": "2.0",
    "description": "Convert amount in one currency to another.",
    "browser_action": {
        "default_popup": "popup.html",
        "icons": {
            "16": "icons/icon-16.png",
            "48": "icons/icon-48.png",
            "128": "icons/icon-128.png"
        }
    },
    "permissions": [
        "activeTab"
    ]
}