{
    "manifest_version": 2,
    "name": "Currency Converter",
    "version": "1.0",
    "description": "Convert amount in one currency to another.",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "permissions": [
        "activeTab"
    ]
}